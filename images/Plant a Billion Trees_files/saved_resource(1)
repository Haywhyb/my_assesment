<style>
  /*sticky footer styles */
#sf-donate-container{display:none;position:fixed;z-index:2147483}@media only screen and (max-width:47em){#sf-donate-container{display:unset;position:fixed;width:100%;bottom:0;left:0;margin:0;z-index:2147483}#sf-donate-x-container{display:-webkit-box;display:-ms-flexbox;display:flex;-webkit-box-pack:end;-ms-flex-pack:end;justify-content:flex-end;margin:0}#sf-donate-x-image{display:block}#sf-donate-x-image:hover{cursor:pointer}#sf-donate-bar{display:-webkit-box;display:-ms-flexbox;display:flex;-webkit-box-pack:end;-ms-flex-pack:end;justify-content:flex-end;background-color:#fff;-webkit-box-shadow:0 10px 30px 0 rgba(0,0,0,.25);box-shadow:0 10px 30px 0 rgba(0,0,0,.25)}#sf-donate-text{-webkit-box-flex:1;-ms-flex-positive:1;flex-grow:1;text-align:center;height:55px;line-height:60px;font-family:'Whitney A','Whitney B',sans-serif;font-weight:500;font-size:16px;color:#3c3c3c}#ye19_stickyfooter_button{margin:10px 10px 10px 0}}@media only screen and (max-width:320px){#sf-donate-text{font-size:14px}}

 /* countdown styles*/
#countdown-clock{width:276px;font-family:'Whitney A','Whitney B',sans-serif;display:flex;justify-content:space-between;margin:0 0 20px -15px}#countdown-clock .countdown-item{position:relative;width:65px;height:54px;color:#000;font-weight:600;text-align:center;font-size:30px;line-height:36px}#countdown-clock .countdown-item:before{content:":";position:absolute;left:-8px;top:-2px;font-weight:400}#countdown-clock .countdown-item:after{position:absolute;left:5px;bottom:0;color:#3C3C3C;font-size:12px;font-weight:200;line-height:15px;text-transform:uppercase}#countdown-clock .countdown-item:nth-of-type(1):after{content:"days";left:18px}#countdown-clock .countdown-item:nth-of-type(1):before{content:''}#countdown-clock .countdown-item:nth-of-type(2):after{content:"hours";left:12px}#countdown-clock .countdown-item:nth-of-type(3):after{content:"minutes"}#countdown-clock .countdown-item:nth-of-type(4):after{content:"seconds"} .c-pop-up .c-pop-up__content{text-align:left}
</style>

<script>

  function countdownClock(theTime) {
     $( '<div id="countdown-clock"></div>' ).insertAfter( ".c-pop-up__excerpt" );

     var divOpen = '<div class="countdown-item">';
     var divClose = '</div>';
     var $clock = $('#countdown-clock');

     var date = moment.tz(theTime, 'US/Eastern');

     $clock.countdown(date.toDate());
     $clock.bind('update.countdown', function(event) {
         $(this).html(event.strftime(
           divOpen + '%D' + divClose +
           divOpen + '%H' + divClose +
           divOpen + '%M' + divClose +
           divOpen + '%S' + divClose));
       });

     $clock.bind('finish.countdown', function(event) {$(this).hide()});
   }

  //If C53 Pop Up is not present on page, do not show sticky footer:
  var pop_up = document.getElementsByClassName("c-pop-up__container-inner");
  if(pop_up.length == 0){
    document.getElementById("sf-emailsignup-container").style.display = "none";
  } else if (pop_up) {

    // if popup is being used, set countdown - UPDATE TIME HERE
    countdownClock("2020-01-02 03:00");

    $( "#sf-donate-x-image" ).one("click", function(){
      $("#sf-donate-container").animate({
        bottom: "-300px"
      }, 1000, function() {
        //Animation complete
      });
    });

  }

</script>
</div>
